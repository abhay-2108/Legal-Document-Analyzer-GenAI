{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./",
    "MODULE_NAME": "main",
    "VARIABLE_NAME": "app"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}
